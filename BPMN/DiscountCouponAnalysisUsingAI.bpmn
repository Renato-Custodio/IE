        <camunda:connector>
          <camunda:inputOutput>
            <camunda:inputParameter name="method">GET</camunda:inputParameter>
            <camunda:inputParameter name="url">http://&lt;KONG_URL&gt;:8000/discountCoupon/loyaltyCard/${LoyaltyCardID}</camunda:inputParameter>
